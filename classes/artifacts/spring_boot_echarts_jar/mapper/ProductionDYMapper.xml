<?xml version="1.0" encoding="UTF-8"?>
        <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.demo.mapper.ProductionDYMapper">
<select id="findDY" resultType="ProductionDY">
SELECT
MIN(id)as id,
name,
SUM(num) AS num
FROM
c
GROUP BY `name`
</select>

</mapper>